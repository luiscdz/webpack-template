{
  "name": "template",
  "version": "1.0.0",
  "description": "template para usar webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract/webpack.config.js",
    "build:multi:css": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:dev": "webpack --config ./webpack-dev-server/webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^0.28.11",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "npm": "^6.1.0"
  }
}
